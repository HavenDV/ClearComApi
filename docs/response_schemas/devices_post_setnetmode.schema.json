{
  "$schema": "http://json-schema.org/schema#",
  "title": "Set Network Mode",
  "type": "object",
  "required": [
    "mode"
  ],
  "properties": {
    "mode": {
      "type": "string",
      "enum": [
        "LAN",
        "WAN"
      ],
      "description": "LQ only. Configures the tunneling mode. LAN is less resource intensive but will not work with routers."
    }
  },
  "additionalProperties": false
}
